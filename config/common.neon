parameters:


application:
	errorPresenter: Error
	mapping:
		*: App\*Module\Presenters\*Presenter


session:
	expiration: 14 days


di:
	export:
		parameters: no
		tags: no

extensions:
    translation: Contributte\Translation\DI\TranslationExtension

translation:
	locales:
		whitelist: [en_US, cs]
		default: en_US
		fallback: [en_US]
	dirs:
		- %appDir%/langs
	returnOriginalMessage: true # to not translate undefined messages, default is true
